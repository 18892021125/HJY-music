  box-sizing: border-box;
  letter-spacing: 0;
  word-wrap: break-word;
}
/* 设置常用元素尺寸规则 */
view, textarea, input, label, form, button, image {
  box-sizing: border-box;
}
/* 按钮样式处理 */
button {
  font-size: 28rpx;
}
/* 取消按钮默认的边框线效果 */
button:after {
  border: none;
}
/* 设置图片默认样式，取消默认尺寸 */
image {
  display: block;
  height: auto;
  width: auto;
