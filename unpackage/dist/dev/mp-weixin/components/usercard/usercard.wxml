<view>
    <block wx:for="{{prodata.list}}" wx:for-item="val" wx:for-index="index" wx:key="index">
        <view data-event-opts="{{[['tap',[['goUrl',['./pages/user/user']]]]]}}" class="followuser" bindtap="__e">
            <view class="followitem">
                <block wx:if="{{val.src}}">
                    <image style="width:70px;height:70px;margin-top:15rpx;margin-left:15rpx;" src="{{val.src}}"></image>
                </block>
                <view class="followinfo">
                    <view class="_br"></view>
                    <block wx:if="{{val.name}}">
                        <font class="name" vue-id="{{'42a6c5bc-1-'+index}}" bind:__l="__l" vue-slots="{{['default']}}">{{val.name}}</font>
                    </block>
                    <block wx:if="{{val.sign}}">
                        <font class="sign" vue-id="{{'42a6c5bc-2-'+index}}" bind:__l="__l" vue-slots="{{['default']}}">{{val.sign}}</font>
                    </block>
                </view>
                <view class="moreinfoicon">
                    <uni-icons vue-id="{{'42a6c5bc-3-'+index}}" custom-prefix="iconfont" type="icon-moreinfo" size="25" data-event-opts="{{[['^click',[['playmusic']]]]}}" bind:click="__e" bind:__l="__l"></uni-icons>
                </view>
            </view>
        </view>
    </block>
</view>